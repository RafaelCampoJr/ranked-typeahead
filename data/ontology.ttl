# The ontology data
prefix net: <http://stardog.com/network/>
prefix scope: <http://stardog.com/network/classes/>

net:Router a rdfs:Class ; 
        rdfs:label "Router" .

scope:Regional rdfs:subClassOf net:Router .
scope:Local rdfs:subClassOf net:Router .        

net:connects a owl:SymmetricProperty ;
    rdfs:label "connects" ;
    rdfs:domain net:Router ;
    rdfs:range net:Router .   

    