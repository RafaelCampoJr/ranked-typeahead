@prefix test: <tag:stardog:api:test:> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 



test:defaults
    test:server "http://localhost:5820" ;
    test:database "router" ;
    test:warmups 2;
    test:runs 3 .     



## Gets all variable ?routers defined as net:Router
## REQUIRES REASONING
##
test:getsVariableRoutersDefinedAsRouter a test:Test ;
    test:resultFile "../query-results/getsVariableRoutersDefinedAsRouter.json" ;
    test:queryFile "../query-files/getsVariableRoutersDefinedAsRouter.sparql" ;
    test:reasoning "true"^^xsd:boolean .



## Gets the rounded number of edge connections per node grouped by the router type (i.e. Local or Regional router type)
##
test:numberOfConnectionsPerNodeGroupedByRouterType a test:Test ;
    test:resultFile "../query-results/numberOfConnectionsPerNodeGroupedByRouterType.json" ;
    test:queryFile "../query-files/numberOfConnectionsPerNodeGroupedByRouterType.sparql" ;
    test:reasoning "false"^^xsd:boolean .



## Gets edges regardless of direction. Must add --reasoning to the query
## REQUIRES REASONING
##
test:getsEdgesRegardlessOfDirection a test:Test ;
    test:resultFile "../query-results/getsEdgesRegardlessOfDirection.json" ;
    test:queryFile "../query-files/getsEdgesRegardlessOfDirection.sparql" ;
    test:reasoning "true"^^xsd:boolean .



## Node index directional
## Omit reasoning
test:nodeIndexDirectional a test:Test ;
    test:resultFile "../query-results/nodeIndexDirectional.json" ;
